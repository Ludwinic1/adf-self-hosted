{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-self-hosted-test"
		},
		"LS_ADLS_ADF_SELF_HOSTED_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsadfselfhosted.dfs.core.windows.net"
		},
		"LS_ADLS_OUTPUT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsadfselfhostedoutput.dfs.core.windows.net"
		},
		"TR_STORAGE_EVENT_TEST_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/126c93ce-f0a6-493c-8991-cdcbd797366d/resourceGroups/rg-adf-selfhosted-test/providers/Microsoft.Storage/storageAccounts/adlsadfselfhosted"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-self-hosted-test_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_ADF_SELF_HOSTED_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_ADF_SELF_HOSTED_properties_typeProperties_url')]"
					},
					"LS_ADLS_OUTPUT_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_OUTPUT_properties_typeProperties_url')]"
					},
					"TR_STORAGE_EVENT_TEST_properties_typeProperties_scope": {
						"value": "[parameters('TR_STORAGE_EVENT_TEST_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}